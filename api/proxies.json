{
  "$schema": "https://json.schemastore.org/proxies",
  "proxies": {
    "Callback": {
      "matchCondition": {
        "methods": ["GET"],
        "route": "/api/Callback"
      },
      "backendUri": "http://localhost/api/AuthenticateCallback",
      "requestOverrides": {
        "backend.request.querystring.code": "",
        "backend.request.querystring._code": "{request.querystring.code}"
      }
    }
  }
}